#!/usr/bin/env bash

export CDPATH=$HOME/projects/lifen
export EDITOR=code
export GITHUB_NPM_TOKEN=$(security find-generic-password -s "GITHUB_NPM_TOKEN" -w)
export HOMEBREW_NO_ANALYTICS=1
export LANG="en_US.UTF-8"
export LANGUAGE="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"
export NVM_DIR="$HOME/.nvm"
export OVERCOMMIT_DISABLE=1
export PATH="$HOME/bin:./bin:./node_modules/.bin:${PATH}:/usr/local/sbin"
export ZSH=~/.oh-my-zsh
