#!/usr/bin/env bash

export ANTHROPIC_AUTH_TOKEN=$(security find-generic-password -a "$USER" -s "ANTHROPIC_AUTH_TOKEN" -w 2>/dev/null)
export ANTHROPIC_BASE_URL="https://litellm.private.ia-prod.lifen.fr/"
export CDPATH=$HOME/projects/lifen
export EDITOR=code
export GITHUB_NPM_TOKEN=$(security find-generic-password -s "GITHUB_NPM_TOKEN" -w 2>/dev/null)
export GPG_TTY=$(tty)
export HOMEBREW_NO_ANALYTICS=1
export LANG="en_US.UTF-8"
export LANGUAGE="en_US.UTF-8"
export LC_ALL="en_US.UTF-8"
export NVM_DIR="$HOME/.nvm"
export OBJC_DISABLE_INITIALIZE_FORK_SAFETY=YES
export OVERCOMMIT_DISABLE=1
export PATH="$HOME/bin:./bin:./node_modules/.bin:${PATH}:/usr/local/sbin"
export ZSH=~/.oh-my-zsh
